# AWS Configuration
aws_region  = "me-central-1"
environment = "production"

# VPC Configuration
vpc_cidr           = "10.0.0.0/16"
availability_zones = ["me-central-1a", "me-central-1b"]
private_subnets    = ["10.0.1.0/24", "10.0.2.0/24"]
public_subnets     = ["10.0.101.0/24", "10.0.102.0/24"]

# RDS Configuration
db_instance_class    = "db.t3.medium"
db_allocated_storage = 100
db_engine_version    = "15.4"
db_name              = "real_estate_prod"
db_username          = "postgres"
db_password          = "CHANGE_ME_SECURE_PASSWORD"

# OpenSearch Configuration
opensearch_instance_type   = "t3.medium.search"
opensearch_instance_count  = 2
opensearch_ebs_volume_size = 100
opensearch_master_user     = "admin"
opensearch_master_password = "CHANGE_ME_SECURE_PASSWORD"

# ECS Configuration
ecs_task_cpu      = 1024
ecs_task_memory   = 2048
ecs_desired_count = 2
container_image   = "ACCOUNT_ID.dkr.ecr.me-central-1.amazonaws.com/dubai-real-estate-backend:latest"

# Application Configuration
jwt_secret = "CHANGE_ME_MIN_32_CHARS_JWT_SECRET"
